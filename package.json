{
  "name": "supabase-vector-search",
  "version": "1.0.0",
  "description": "Experimental vector similarity search for Supabase with PostgreSQL and pgvector. Supports multi-vector weighted search for advanced semantic matching.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "supabase",
    "examples",
    "docs",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"examples/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"examples/**/*.ts\" \"tests/**/*.ts\"",
    "example:document-search": "npx ts-node examples/document-search.ts",
    "example:resume-search": "npx ts-node examples/resume-search.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "supabase",
    "vector-search",
    "semantic-search",
    "pgvector",
    "postgresql",
    "embeddings",
    "openai",
    "cohere",
    "similarity-search",
    "machine-learning",
    "typescript",
    "database",
    "search-engine",
    "resume-search",
    "candidate-matching"
  ],
  "author": "Ravindra Kanchikare (krhebber)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/krhebber/supabase-vector-search.git"
  },
  "homepage": "https://github.com/krhebber/supabase-vector-search#readme",
  "bugs": {
    "url": "https://github.com/krhebber/supabase-vector-search/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@supabase/supabase-js": "^2.0.0",
    "openai": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    }
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/ui": "^4.0.4",
    "eslint": "^8.50.0",
    "openai": "^4.24.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0",
    "vitest": "^4.0.4"
  }
}
